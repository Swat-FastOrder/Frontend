@import '../../stylus/variables.styl'

.kitchen
  position: absolute
  z-index: 1
  width: 100%
  &__orders
    padding: 20px
  &__modals
    height: 100%
    width: 100%
    display: flex
    justify-content: center
    align-items: center
    position: fixed
    top: 0
    right: 0
    z-index: 25
    background-color: rgba(80, 133, 173, .5)
    transition: all 0.3s linear
